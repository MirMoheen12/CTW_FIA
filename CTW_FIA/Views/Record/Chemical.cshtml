function ViewChemialById(x)
    {
        var url = "/api/Chemicals/GetChemicalById?id="+x;
            //alert(url);
            $.ajax({  
            type: "GET",  
            url: url,
            dataType: 'json',
            contentType: 'application/json; charset=utf-8',
            success: function (response) {  
              //alert(JSON.stringify(response));
                //alert((response.strURN.toString()));
                  //alert(JSON.stringify(response));
                     $("#chemName").val(response.name);
                     $("#otherchemName").val(response.otherNames);
                     $("#chemRecovery").val(response.countryRecovered);
                     $("#chemOrigin").val(response.countryOrigin);
                     $("#chemCASNo").val(response.casNo);
                     $("#chemrtces").val(response.rtecsNo);
                     $("#unNo").val(response.unNo);
                     $("#ecNo").val(response.ecNo);
                     $("#chemicalFormula").val(response.chemicalFormula);
                     $("#physicalAppearance").val(response.physicalAppearance);
                     $('#chemicalDangers').val(response.chemicalDangers);
                     $("#routesOfExposure").val(response.routesOfExposure);
                     $("#chemTLV").val(response.occupationalExposureLimitsTLV);
                     $("#chemMAK").val(response.occupationalExposureLimitsMAK);
                     $("#spillageDisposal").val(response.spillageDisposal);
                     $("#chemPackaging").val(response.packaging);
                     $("#chemLabelling").val(response.labelling);
                     $("#safeStorage").val(response.safeStorage);
                     $("#transportEmergencyCard").val(response.transportEmergencyCard);
                     $("#nfpaCode").val(response.nfpaCode);
                     $("#unHazardCard").val(response.unHazardClass);
                     $("#unSubsidiaryClass").val(response.unSubsidiaryRisks);
                     $("#unPackGroup").val(response.unPackGroup);
                     $("#hazardExposure").val(response.hazardExposure);
                     $("#lowBP").val(response.boilingPointLow);
                     $("#highBP").val(response.boilingPointHigh);
                     $("#lowMP").val(response.meltingPointHighLow);
                     $("#highMP").val(response.meltingPointHigh);
                     $("#lowdensity").val(response.densityLow);
                     $("#highdensity").val(response.densityHigh);
                     $("#lowrelativeDensity").val(response.relativeDensityLow);
                     $("#highrelativeDensity").val(response.relativeDensityHigh);
                     $("#solubilityinWater").val(response.waterSolubility);
                     $("#vapourPressureLow").val(response.vapourPressureLow);
                     $("#vapourPressureHigh").val(response.vapourPressureHigh);
                     $("#vapourlowDensity").val(response.relativeVapourDensityLow);
                     $("#vapourhighDensity").val(response.relativeVapourDensityHigh);
                     $("#flashPointLow").val(response.flashPointLow);
                     $("#flashPointHigh").val(response.flashPointHigh);
                     $("#autoIgnitionLow").val(response.autoIgnitionTempLow);
